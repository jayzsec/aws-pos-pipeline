{
  "name": "pos-system",
  "version": "0.1.0",
  "bin": {
    "pos-system": "bin/pos-system.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
