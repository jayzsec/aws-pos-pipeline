    "synth:dev": "cdk synth PosSystem-dev --context environment=dev",
    "synth:stage": "cdk synth PosSystem-stage --context environment=stage",
    "synth:prod": "cdk synth PosSystem-prod --context environment=prod"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "20.4.2",
    "aws-cdk": "2.88.0",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "aws-cdk-lib": "2.88.0",
    "constructs": "^10.2.69",
    "source-map-support": "^0.5.21"
